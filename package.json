    "@pinia/testing": "^0.1.3",
    "@sidebase/nuxt-auth": "latest",
    "@vitest/web-worker": "^1.6.0",
    "@vue/test-utils": "^2.4.5",
    "firebase": "^10.11.1",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^4.9.0",
    "next-auth": "4.21.1",
    "nuxt": "^3.11.2",
    "pinia": "^2.1.7",
    "sass": "^1.75.0",
